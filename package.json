{
  "name": "cassettator.js",
  "version": "0.69.420",
  "description": "A collection of video.js components and plugins",
  "author": "amtins <amtins.dev@gmail.com>",
  "license": "MIT",
  "keywords": [
    "video.js",
    "video",
    "player",
    "markers",
    "chapters",
    "cassette",
    "cassettator",
    "plugin"
  ],
  "bugs": {
    "url": "https://github.com/amtins/cassettator.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amtins/cassettator.js.git"
  },
  "main": "dist/index.js",
  "global": "dist/cassettator.global.js",
  "cassettator-markers-css": "dist/cassettator.markers.css",
  "browserslist": "defaults",
  "targets": {
    "cassettator": {
      "source": "src/index.js",
      "distDir": "./dist",
      "isLibrary": true,
      "includeNodeModules": false,
      "outputFormat": "esmodule",
      "optimize": true
    },
    "global": {
      "source": "src/browser.js",
      "distDir": "./dist",
      "isLibrary": false,
      "includeNodeModules": true,
      "outputFormat": "global",
      "optimize": true
    },
    "cassettator-markers-css": {
      "source": "src/markers/css/cassettator-markers.css",
      "distDir": "./dist",
      "isLibrary": false,
      "optimize": true
    }
  },
  "scripts": {
    "build": "npm run build:cassettator && npm run build:browser && npm run build:css",
    "build:cassettator": "parcel build --target cassettator",
    "build:browser": "parcel build --target global",
    "build:css": "parcel build --target cassettator-markers-css",
    "builds": "parcel build",
    "start": "parcel -p 6969 src/**/*.html"
  },
  "files": [
    "/dist",
    "/docs",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "video.js": ">=7"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "parcel": "^2.10.3"
  }
}
